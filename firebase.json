{
  "hosting": {
    "headers": [
      { 
        "source":"/service-worker.js", 
        "headers": [
          {
            "key": "Cache-Control", 
            "value": "1800"
          }
        ]
      }
    ],
    "site": "prueba-prixz",
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
